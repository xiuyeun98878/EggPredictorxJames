content.ZIndex = 2

-- Button creation function with improved styling
local function updateStop(btn)
    btn.BackgroundColor3 = autoStopOn and BUTTON_GREEN or BUTTON_RED
    btn.Text = "[A] Auto Stop: " .. (autoStopOn and "ON" or "OFF")
end

local function makeButton(txt, yPos, color, hoverColor, onClick)
    local button = Instance.new("TextButton", content)
    button.Size = UDim2.new(1, 0, 0, 32)
    button.Position = UDim2.new(0, 0, 0, yPos)
    button.BackgroundColor3 = color
    button.Text = txt
    button.Font = Enum.Font.GothamSemibold
    button.TextColor3 = Color3.new(1, 1, 1)
    button.TextSize = 14
    button.TextStrokeTransparency = 0.25
    button.ZIndex = 3
    
    local buttonCorner = Instance.new("UICorner", button)
    buttonCorner.CornerRadius = UDim.new(0, 8)
    
    local buttonStroke = Instance.new("UIStroke", button)
    buttonStroke.Color = BROWN_BORDER
    buttonStroke.Thickness = 2
    
    button.MouseEnter:Connect(function()
        button.BackgroundColor3 = hoverColor
        local tween = TweenService:Create(button, TweenInfo.new(0.2), {TextColor3 = Color3.fromRGB(245, 245, 245)})
        tween:Play()
    end)
    
    button.MouseLeave:Connect(function()
        button.BackgroundColor3 = color
        local tween = TweenService:Create(button, TweenInfo.new(0.2), {TextColor3 = Color3.new(1, 1, 1)})
        tween:Play()
    end)
    
    if onClick then
        button.MouseButton1Click:Connect(onClick)
    end
    
    return button
end

-- Create buttons with improved spacing
local stopButton = makeButton("[A] Auto Stop: OFF", 0, BUTTON_RED, BUTTON_RED_HOVER, function()
    autoStopOn = not autoStopOn
    updateStop(stopButton)
end)

local rerollButton = makeButton("[B] Reroll Pet Display", 38, BUTTON_BLUE, BUTTON_BLUE_HOVER, function()
    for _, data in pairs(displayedEggs) do
        local pet = getNonRepeatingRandomPet(data.eggName, data.lastPet)
        if pet and data.label then
            data.label.Text = data.eggName .. " | " .. pet
            data.lastPet = pet
        end
    end
end)

local infoButton = makeButton("[?] Info", 76, BUTTON_GRAY, Color3.fromRGB(210, 210, 210), function()
    print("Egg Randomizer Info: This tool shows randomized pet names for your eggs!")
end)

